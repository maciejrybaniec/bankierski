[ignore]

[include]

[libs]

[options]
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable
unsafe.enable_getters_and_setters=true

module.name_mapper='^Store\/\(.*\)$' -> '<PROJECT_ROOT>/store/\1'
module.name_mapper='^Components\/\(.*\)$' -> '<PROJECT_ROOT>/components/\1'
module.name_mapper='^Pages\/\(.*\)$' -> '<PROJECT_ROOT>/pages/\1'
module.name_mapper='^Middleware\/\(.*\)$' -> '<PROJECT_ROOT>/middleware/\1'
module.name_mapper='^Styles\/\(.*\)$' -> '<PROJECT_ROOT>/styles/\1'

suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
